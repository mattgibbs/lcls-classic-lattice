                FINT=0, E2=-ABh1, FINTX=0.400
  BXh4A : SBEN, TYPE="5D3.9", L=LBh1L, ANGLE=+ABh1L, HGAP=GBh/2, &
                E1=+ABh1, FINT=0.400, FINTX=0
